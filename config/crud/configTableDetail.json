  "Professional": {
    "model": "Professional",
    "title": "Profesionales de la Salud",
    "description": "Gestión de profesionales",
    "columns": [
      {
        "key": "id",
        "title": "ID",
        "type": "text",
        "sortable": true,
        "hideable": true,
        "hidden": true
      },
      {
        "key": "fullName",
        "title": "Nombre completo",
        "type": "text",
        "sortable": true,
        "filterable": true,
        "frozen": true,
        "width": 260,
        "validation": {
          "required": true,
          "minLength": 2,
          "maxLength": 80
        }
      },
      {
        "key": "email",
        "title": "Email",
        "type": "email",
        "sortable": true,
        "filterable": true,
        "width": 220,
        "validation": {
          "required": true,
          "maxLength": 100
        }
      },
      {
        "key": "phone",
        "title": "Teléfono",
        "type": "text",
        "sortable": true,
        "filterable": true,
        "width": 140,
        "hideable": true,
        "validation": {
          "required": true,
          "pattern": "^\\d{7,10}$",
          "patternMessage": "Solo dígitos (7 a 10)."
        }
      },
      {
        "key": "specialty",
        "title": "Especialidad",
        "type": "badge",
        "options": [
          "Medicina General",
          "Odontología",
          "Fisioterapia",
          "Psicología",
          "Otro"
