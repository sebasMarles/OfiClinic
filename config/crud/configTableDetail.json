        "sortable": true,
        "hideable": true,
        "width": 180
      }
    ],
    "rowActions": [
      {
        "id": "view",
        "label": "Ver",
        "icon": "eye",
        "variant": "ghost",
        "action": "view"
      },
      {
        "id": "edit",
        "label": "Editar",
        "icon": "pencil",
        "variant": "ghost",
        "action": "edit"
      },
      {
        "id": "delete",
        "label": "Eliminar",
        "icon": "trash",
        "variant": "destructive",
        "action": "delete",
        "confirmMessage": "¿Eliminar este profesional?"
      }
    ],
    "bulkActions": [
      {
        "id": "delete",
